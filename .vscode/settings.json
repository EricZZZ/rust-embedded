{
  // 这会告诉 rust-analyzer 使用 cargo check 命令，并带上 --target 参数
  "rust-analyzer.check.command": "cargo",
  "rust-analyzer.check.extraArgs": [
    "--target",
    "riscv32imc-unknown-none-elf"
    // 将上面的 target 替换成你项目实际使用的目标，比如 "xtensa-esp32-none-elf"
  ],
  // 禁用 rust-analyzer 检查测试 crate，因为 embedded 项目通常没有 test
  "rust-analyzer.rustc.source-extra-args": ["--cfg", "no_cargo_test"]
}
